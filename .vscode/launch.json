{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Expo (Web)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8081",
      "webRoot": "${workspaceFolder}",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug Expo (Android)",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ensismoebius/workspaces/react-workspace/languid/serverAPI/exercises",
      "program": "/home/ensismoebius/workspaces/react-workspace/languid/serverAPI/exercises/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}