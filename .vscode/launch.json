{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Expo (Web)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8081",
      "webRoot": "${workspaceFolder}",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug Expo (Android)",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    }
  ]
}